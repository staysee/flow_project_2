<% provide(:title, "Home") %>

<div class="center jumbotron">
<h1>Welcome to Flow Finder</h1>

<p> This is the home page for Flow Finder </p>


<% flash.each do |key,value|  %>
    <div class="<%= key %>" > <%= value %> </div>
<% end %>


<% if current_user  %>
  <%=link_to "My Profile Page", user_path %>
  <%= link_to "Logout", destroy_session_path %>
<% else %>
    <%= link_to "Register", new_user_path, class: "btn btn-lg btn-primary"  %>
<% end %>

